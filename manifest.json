{
  "name": "OAS.LPU TOOL",
  "version": "1.0",
  "description": "OAS.LPU TOOL for result analysis",
  "author": "Saurav Hathi",
  "manifest_version": 3,
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": ["scripting", "activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://oas.lpu.in/*"],
      "css": ["popup.css"],
      "js": ["content.js", "JqueryVal.js"],
      "run_at": "document_idle"
    }
  ]
}